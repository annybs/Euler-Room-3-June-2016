d'voc $ cf $ slow 4 $ ss'verse
  # vowel "e"

d'voc $ cf $ slow 4 $ -- then slow 2, then no slow @ end
--  slowspread ($) [id, (within (0.25, 0.5) $ const silence), (# speed "-1")] $ -- @ end
