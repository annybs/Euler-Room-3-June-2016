msil [d'fly,d'bz,d'shard,d'misc,d'ant,d'cymb]

d'misc $ overlay bass'5f $
  (0.75 ~>) $ bass'4

d'drum $ sound "[k*2 [sn ~ ~ k] [k k*2] sn]/4"

d'shard $ whenmod 7 3 (density (5%4)) $ plox'4 "[ice plode]*2"
d'fly $ fly'5 True
d'ant $ bass2'6
d'cymb $ wtsn'6

d'gnome $ gnome'6 3

d'bz $ within (0.5, 1) (density 2) $
